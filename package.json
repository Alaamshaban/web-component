{
    "name": "navigation-ui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --port 4200 --proxy-config proxy_conf.json",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "lint:ci": "npm run lint",
        "e2e": "ng e2e",
        "build:nav": "ng build nav",
        "pack:nav": "ng build --prod nav && npm run copy && cd dist/nav && npm pack",
        "precommit": "npm run lint",
        "prepush": "npm run lint:ci",
        "try-dev": "ng build nav && npm start",
        "copy": "cp -r src/i18n dist/nav/lib && cp -r projects/nav/src/lib/assets dist/nav/lib",
        "try": "ng build --prod nav && npm run copy && cd dist/nav && npm pack && ng serve --port 4300 --proxy-config proxy_conf.json",
        "prebuild": "npm run combine",
        "extract-i18n": "bash extract-i18n.sh",
        "combine": "ngx-i18n-combine -i  projects/nav/src/lib/i18n/*.json -o ./src/i18n/*.json",
        "build-element": "ng build nav && ng build --project elements --prod --single-bundle=true --output-hashing=none && node nav-build.element.js"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run lint:ci",
            "pre-commit": "npm run lint:ci"
        }
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/architect": "^0.1100.5",
        "@angular-extensions/elements": "^11.0.1",
        "@angular/animations": "^11.0.4",
        "@angular/cdk": "^11.0.2",
        "@angular/common": "^11.0.4",
        "@angular/compiler": "^11.0.4",
        "@angular/core": "^11.0.4",
        "@angular/elements": "^11.0.4",
        "@angular/flex-layout": "^11.0.0-beta.33",
        "@angular/forms": "^11.0.4",
        "@angular/material": "^11.0.2",
        "@angular/platform-browser": "^11.0.4",
        "@angular/platform-browser-dynamic": "^11.0.4",
        "@angular/router": "^11.0.4",
        "@nguniversal/express-engine": "^11.0.1",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "@webcomponents/webcomponentsjs": "^2.5.0",
        "concat": "^1.0.3",
        "core-js": "^3.8.1",
        "document-register-element": "^1.7.2",
        "flag-icon-css": "^3.5.0",
        "hotkeys-js": "^3.8.1",
        "husky": "^4.3.6",
        "ng-keyboard-shortcuts": "^8.0.0",
        "ngx-build-plus": "^12.2.0",
        "ngx-cookie": "^5.0.0",
        "ngx-i18n-combine": "^1.1.0",
        "rxjs": "^6.6.3",
        "stylelint-cli": "^1.1.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.1.1",
        "@angular/cli": "~11.0.4",
        "@angular/compiler-cli": "^11.0.4",
        "@angular/language-service": "^11.0.4",
        "@biesbjerg/ngx-translate-extract": "^4.2.0",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.12.47",
        "codelyzer": "^6.0.1",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^11.0.0",
        "protractor": "~7.0.0",
        "stylelint": "^13.13.1",
        "ts-node": "~5.0.1",
        "tslint": "~6.1.0",
        "typescript": "~4.0.5"
    }
}
