{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "nav": {
            "root": "projects/nav",
            "sourceRoot": "projects/nav/src",
            "projectType": "library",
            "prefix": "cloudinn",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/nav/tsconfig.lib.json",
                        "project": "projects/nav/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "project": "projects/nav/ng-package.prod.json",
                            "tsConfig": "projects/nav/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/nav/src/test.ts",
                        "tsConfig": "projects/nav/tsconfig.spec.json",
                        "karmaConfig": "projects/nav/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/nav/tsconfig.lib.json",
                            "projects/nav/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "elements": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/elements",
            "sourceRoot": "projects/elements/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "ngx-build-plus:browser",
                    "options": {
                        "outputPath": "dist/elements/browser",
                        "index": "projects/elements/src/index.html",
                        "main": "projects/elements/src/main.ts",
                        "preserveSymlinks": true,
                        "polyfills": "projects/elements/src/polyfills.ts",
                        "tsConfig": "projects/elements/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/elements/src/favicon.ico",
                            "projects/elements/src/assets"
                        ],
                        "styles": [
                            "projects/elements/src/styles.scss"
                        ],
                        "scripts": [{
                                "bundleName": "polyfill-webcomp-es5",
                                "input": "node_modules/@webcomponents/webcomponentsjs/custom-elements-es5-adapter.js"
                            },
                            {
                                "bundleName": "polyfill-webcomp",
                                "input": "node_modules/@webcomponents/webcomponentsjs/bundles/webcomponents-sd-ce-pf.js"
                            }
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "projects/elements/src/environments/environment.ts",
                                "with": "projects/elements/src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ]
                        }
                    },
                    "serve": {
                        "builder": "ngx-build-plus:dev-server",
                        "options": {
                            "browserTarget": "elements:build"
                        },
                        "configurations": {
                            "production": {
                                "browserTarget": "elements:build:production"
                            }
                        }
                    },
                    "extract-i18n": {
                        "builder": "@angular-devkit/build-angular:extract-i18n",
                        "options": {
                            "browserTarget": "elements:build"
                        }
                    },
                    "test": {
                        "builder": "ngx-build-plus:karma",
                        "options": {
                            "main": "projects/elements/src/test.ts",
                            "polyfills": "projects/elements/src/polyfills.ts",
                            "tsConfig": "projects/elements/tsconfig.spec.json",
                            "karmaConfig": "projects/elements/karma.conf.js",
                            "assets": [
                                "projects/elements/src/favicon.ico",
                                "projects/elements/src/assets"
                            ],
                            "styles": [
                                "projects/elements/src/styles.scss"
                            ],
                            "scripts": []
                        }
                    },
                    "lint": {
                        "builder": "@angular-devkit/build-angular:tslint",
                        "options": {
                            "tsConfig": [
                                "projects/elements/tsconfig.app.json",
                                "projects/elements/tsconfig.spec.json",
                                "projects/elements/e2e/tsconfig.json"
                            ],
                            "exclude": [
                                "**/node_modules/**"
                            ]
                        }
                    },
                    "e2e": {
                        "builder": "@angular-devkit/build-angular:protractor",
                        "options": {
                            "protractorConfig": "projects/elements/e2e/protractor.conf.js",
                            "devServerTarget": "elements:serve"
                        },
                        "configurations": {
                            "production": {
                                "devServerTarget": "elements:serve:production"
                            }
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "nav-ui",
    "cli": {
        "analytics": false
    }
}